{
  "name": "multi-tenant-demo-app--client",
  "description": "Single-page React app with multi-tenancy enabled.  This is to demonstrate how developers can intergrate their multi-tenant app with the Wristband platform.",
  "author": "Wristband",
  "version": "0.5.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.12.1",
    "npm": ">=8.19.2"
  },
  "scripts": {
    "start": "vite --host",
    "build": "rm -rf dist/; rm -rf ../server/dist/; vite build; mv \"./dist/\" \"../server/dist/\"",
    "serve": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.126",
    "@mui/material": "^5.15.6",
    "@mui/x-date-pickers": "^6.1.0",
    "@tanstack/react-query": "^4.29.1",
    "@tanstack/react-query-devtools": "^4.29.1",
    "axios": "^1.6.7",
    "date-fns": "^2.29.3",
    "material-ui-phone-number": "^3.0.0",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-number-format": "^5.1.4",
    "react-router-dom": "^6.2.1",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.34.1",
    "prettier": "^2.8.8",
    "typescript": "^5.3.3",
    "vite": "^5.3.3",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
